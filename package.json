{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"docker-compose up mongo\" \"npm-watch graph-type-gen\" \"next dev\"",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "NEXT_PUBLIC_WEBSITE_URL=\"https://www.prescottjr.com\" next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "lint": "next lint",
    "graph-type-gen": "graphql-codegen --config codegen.yml",
    "test:types": "tsc"
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@notionhq/client": "^1.0.2",
    "@reonomy/react-confetti-explosion": "^1.0.3",
    "apollo-server-micro": "^3.0.1",
    "fast-memoize": "^2.5.2",
    "graphql": "^15.5.1",
    "mermaid": "^8.14.0",
    "micro": "^9.3.4",
    "mongodb": "^4.0.1",
    "next": "^12.1.0",
    "next-auth": "^3.27.3",
    "next-sitemap": "^2.5.10",
    "node-fetch": "^3.2.3",
    "notion-client": "^4.16.1",
    "notion-types": "^4.16.0",
    "react": "^17.0.2",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-modal": "^3.14.3",
    "react-notion-x": "^4.16.2",
    "react-spring": "^9.2.4",
    "styled-components": "^5.3.0",
    "swr": "^0.5.6",
    "ts-essentials": "^7.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@graphql-codegen/typescript-resolvers": "1.20.0",
    "@types/mermaid": "^8.2.8",
    "@types/react": "17.0.15",
    "@types/react-modal": "^3.12.1",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "concurrently": "^6.2.0",
    "eslint": "7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-watch": "^0.10.0",
    "prettier": "^2.3.2",
    "typescript": "^4.6.2"
  },
  "watch": {
    "graph-type-gen": {
      "patterns": [
        "*"
      ],
      "extensions": "graphql,yml",
      "quiet": false
    }
  }
}
